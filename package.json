{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "prepare": "bun .husky/install.js",
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "fmt": "prettier . --write",
    "fmt.check": "prettier . --check",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:md": "eslint \"**/*.md?(x)\" --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content --cache-location=.eslintmdcache",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "run-s lint:md lint:js lint:css"
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "bun-plugin-tailwind": "0.0.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.477.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "4.0.12",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@happy-dom/global-registrator": "17.4.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@playwright/test": "1.51.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/bun": "1.2.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.22.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "stylelint": "16.15.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-selector-bem-pattern": "4.0.1",
    "typescript-eslint": "8.26.0"
  }
}
