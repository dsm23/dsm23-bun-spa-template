{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "fmt": "prettier . --write",
    "fmt.check": "prettier . --check",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:md": "eslint \"**/*.md?(x)\" --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content --cache-location=.eslintmdcache",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "run-p lint:md lint:js lint:css"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.4",
    "bun-plugin-tailwind": "0.1.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@commitlint/types": "20.2.0",
    "@eslint/js": "9.39.2",
    "@happy-dom/global-registrator": "20.0.11",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@playwright/test": "1.57.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.3.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "fast-check": "4.5.2",
    "globals": "16.5.0",
    "lefthook": "2.0.13",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-tailwindcss": "0.7.2",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "stylelint-order": "7.0.0",
    "stylelint-selector-bem-pattern": "4.0.1",
    "typed-query-selector": "2.12.0",
    "typescript-eslint": "8.50.1"
  }
}
